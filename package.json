{
  "name": "holly-quintet",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/arianrhodsandlot/Holly-Quintet.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production parcel build \"src/assets/entries/*\"",
    "deploy": "yarn build && yarn start",
    "dev": "cross-env NODE_OPTIONS=\"-r esm\" ts-node src/server",
    "lint": "eslint --ignore-path=.gitignore --ext=js,ts .",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS=\"-r esm\" ts-node src/server",
    "test": "yarn lint && cross-env NODE_OPTIONS=\"-r esm\" ava"
  },
  "dependencies": {
    "chowdown": "^1.2.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "esm": "^3.2.25",
    "express": "~4.17.1",
    "get-port": "^5.0.0",
    "helmet": "^3.20.0",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "material-components-web": "^3.1.0",
    "material-design-icons-iconfont": "^5.0.1",
    "morgan": "^1.9.1",
    "parcel-bundler": "^1.12.3",
    "pug": "2.0.4",
    "request": "^2.88.0",
    "sass": "^1.22.7",
    "serve-favicon": "^2.5.0",
    "ts-node": "^8.3.0",
    "typeface-im-fell-double-pica": "^0.0.72",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/lodash": "^4.14.136",
    "@types/lru-cache": "^5.1.0",
    "@types/morgan": "^1.7.36",
    "@types/parcel-bundler": "^1.12.0",
    "@types/puppeteer": "^1.19.0",
    "@types/request": "^2.48.2",
    "@types/serve-favicon": "^2.2.30",
    "ava": "^2.2.0",
    "cross-env": "^5.2.0",
    "eslint-config-argreion": "gist:86724154ca49ee8b3f9aebae3fe44e0a",
    "puppeteer": "^1.19.0",
    "socks5-https-client": "^1.2.1"
  },
  "config": {
    "port": 1025
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": "last 2 years",
  "eslintConfig": {
    "extends": "argreion"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
